{
  "servers": {
    "Formulize": {
     	"command": "npx",
    	"args": [
        "-y",
        "formulize-mcp"
      ],
      "env": {
        "FORMULIZE_BASE_URL": "https://<your.formulize.site.url>/mcp/",
        "FORMULIZE_API_KEY": "<your api key from your formulize site>"
      }
    }
  }
}
