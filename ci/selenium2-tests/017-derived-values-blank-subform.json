{
  "type": "script",
  "seleniumVersion": "2",
  "formatVersion": 2,
  "steps": [
    {
      "type": "store",
      "text": "/home/travis/build/jegelstaff/formulize",
      "variable": "web_root"
    },
    {
      "type": "store",
      "text": "localhost",
      "variable": "test_domain"
    },
    {
      "type": "store",
      "text": "root",
      "variable": "db_user"
    },
    {
      "type": "store",
      "text": "password",
      "variable": "db_pass"
    },
    {
      "type": "store",
      "text": "formulize",
      "variable": "db_name"
    },
    {
      "type": "store",
      "text": "password",
      "variable": "admin_password"
    },
    {
      "type": "store",
      "text": "short_wait",
      "variable": "234"
    },
    {
      "type": "store",
      "text": "save_wait",
      "variable": "1525"
    },
    {
      "type": "store",
      "text": "return 'This should say ' . \"Person\";",
      "variable": "php_derived_value"
    },
    {
      "type": "get",
      "url": "http://${test_domain}/"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "name",
        "value": "uname"
      },
      "text": "admin"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "name",
        "value": "pass"
      },
      "text": "${admin_password}"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "css selector",
        "value": "input[type=\"submit\"]"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "Admin"
      }
    },
    {
      "type": "pause",
      "waitTime": "2000"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "Stories"
      }
    },
    {
      "type": "pause",
      "waitTime": "2000"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "Value derived from other elements"
      }
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "name",
        "value": "elements-ele_caption"
      },
      "text": "Number of People"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "name",
        "value": "elements-ele_desc"
      },
      "text": "Count of people in subform"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "Options"
      }
    },
    {
      "type": "pause",
      "waitTime": "5000"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "id",
        "value": "formulize-logo"
      }
    },
    {
      "type": "pause",
      "waitTime": "5000"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "id",
        "value": "elements-ele_value"
      }
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "css selector",
        "value": "div > textarea"
      },
      "text": "${php_derived_value}"
    },
    {
      "type": "pause",
      "waitTime": "2000"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "css selector",
        "value": "input.savebutton"
      }
    },
    {
      "type": "pause",
      "waitTime": "2000"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "Tracker"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "Entries in 'Stories'"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "View Screen"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//table[@class='outer']/tbody/tr[4]/td[1]/a"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "id",
        "value": "submitx"
      }
    },
    {
      "type": "verifyTextPresent",
      "text": "This should say Array"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "id",
        "value": "submit_save_and_leave"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "Logout"
      }
    }
  ],
  "data": {
    "configs": {},
    "source": "none"
  },
  "inputs": [],
  "timeoutSeconds": 60
}